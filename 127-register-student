public String registerStudent(String name, String phone, String email) {
    // code here
    apxio__Student__c stud = new apxio__Student__c();
    stud.Name = name;
    stud.apxio__Email__c = email;
    stud.apxio__Phone__c = phone;

    insert stud;
    List<apxio__Student__c> studList = [SELECT Name, apxio__Email__c, apxio__Registration_Number__c 
                                    from apxio__Student__c where Name=:name AND apxio__Email__c =: email]
    return studList[0].apxio__Registration_Number__c;
}