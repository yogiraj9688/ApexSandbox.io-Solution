public String getAccountBillingCityWithSafeNavigation(List<Account> accounts){
   // Code here
   if(!accounts.isEmpty())
   {
       for(Integer i=0; i< accounts.size();i++)
       {
           if(accounts[0] != null)
           {
              return toUpperCase(accounts[0].BillingCity);
           }
           else
           {
               return null;
           }
       }
   }
}
