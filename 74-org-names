public List<String> orgNames(List<String> adjectives, List<String> animals) {
    //code here
    List<String> FinalName = new List<String>();
    Integer i,j;
    for(i=0;i<adjectives.size();i++)
    {
        for(j=0;j<animals.size();j++)
        {
            FinalName.add(adjectives[i]+ '-' + animals[j]);
        }
    }
    
    return FinalName;
}